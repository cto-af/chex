{
  "name": "@cto.af/chex-monorepo",
  "version": "1.0.1",
  "description": "hexdump -C as a small JS library",
  "private": "true",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cto-af/chex.git"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "c8 ava",
    "snaps": "c8 ava --update-snapshots",
    "build": "pnpm --filter '@cto.af/*' --if-present -r build"
  },
  "keywords": [
    "hexdump",
    "hex",
    "unicode"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "devDependencies": {
    "@cto.af/eslint-config": "5.1.12",
    "@types/node": "22.13.5",
    "ava": "6.2.0",
    "c8": "10.1.3",
    "eslint": "9.21.0",
    "eslint-plugin-ava": "15.0.1",
    "eslint-plugin-n": "17.15.1",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "overrides": {
      "fast-glob": "3.3.3",
      "foreground-child": "3.3.1"
    }
  },
  "packageManager": "pnpm@10.5.0",
  "engines": {
    "node": ">=18"
  }
}
